<!-- DTD for INI -->

<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE ini

[

<!ELEMENT section ( comment?, entry* ) >

<!ATTLIST section version CDATA #FIXED "1.0">

<!ELEMENT comment (#PCDATA) >

<!ELEMENT entry (#PCDATA) >

<!ATTLIST entry key CDATA #REQUIRED>

]>
